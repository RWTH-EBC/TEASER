<%namespace file="/modelica_language/" import="get_true_false, get_list, min_orientations"/> <%namespace file="/conversion/" import="deg_to_rad, azmiut_conv"/>
within ${zone.parent.parent.name}.${zone.parent.name}.${zone.parent.name}_DataBase.Swimming_hall_DataBase;
record Swimming_hall_${pool.name} "Swimming_hall_${pool}"
  extends AixLib.DataBase.Pools.IndoorSwimmingPoolBaseRecord(
    TPool = ${pool.temperature},
    VPool = ${pool.volume},
    APool = ${pool.area},
    depthPool = ${pool.depth},
    VStorage = ${pool.volume_storage},
    V_flow_nominal = ${pool.volume_flow},
    V_flow_partial = ${pool.volume_flow_night},
    use_partialLoad = ${get_true_false(pool.use_partial_load)},
    dpHeaExcPool = ${pool.dp_heat_exchanger},
    use_idealHeater = ${get_true_false(pool.use_ideal_heat_exchanger)},
    KHeat = 1,
    THeat = 0.1,
    QMaxHeat = ${pool.QMax},
    QMinHeat = ${pool.Min},
    betaInUse = ${pool.beta_in_use},
    use_poolCover = ${get_true_false(pool.use_pool_cover)},
    use_waterRecycling = ${get_true_false(pool.use_water_recycling)},
    x_recycling = ${pool.x_recycling},
    m_flow_out = ${pool.m_flow_waste_water},
    use_HRS = ${get_true_false(pool.use_heat_recovery)},
    etaHRS = ${pool.eta_heat_recovery},
    use_wavePool = ${get_true_false(pool.use_wave_pool)},
    heightWave = ${pool.wave_height},
    widthWave = ${pool.wave_width},
    periodeWavePul = ${pool.wave_period},
    timeWavePul_start = ${pool.wave_period_start},
    widthWavePul = ${pool.wave_share_period},
    AWalInt = ${pool.area_pool_wall_inner},
    AWalExt = ${pool.area_pool_wall_exterior},
    AFloInt = ${pool.area_pool_floor_inner},
    AFloExt = ${pool.area_pool_floor_exterior},
    hConWaterHorizontal = ${pool.h_con_water_horizontal},
    hConWaterVertical = ${pool.h_con_water_vertical},
    %if pool.pool_wall_construction_type=="concrete_with_insulation":
    PoolWallParam=AixLib.DataBase.Pools.SwimmingPoolWalls.ConcreteInsulationConstruction()
    %elif pool.pool_wall_construction_type=="stainless_steel":
    PoolWallParam=AixLib.DataBase.Pools.SwimmingPoolWalls.StainlessSteelConstruction()
    %else:
    PoolWallParam=AixLib.DataBase.Pools.SwimmingPoolWalls.ConcreteConstruction()
    %endif    );
end Swimming_hall_${pool};